  "fieldInteractionConfig": {
    "activeProfile": "default",
    "profiles": {
      "default": {
        "onChange": {
          "enabled": true,
          "debounce": 300,
          "action": "write_draft"
        },
        "onBlur": {
          "enabled": true,
          "debounce": 0,
          "action": "auto_save"
        },
        "onButtonClick": {
          "enabled": true,
          "debounce": 0,
          "action": "workflow_action"
        }
      }
    }
  },
  "fieldTypes": {
    "Data": {
      "element": "input",
      "props": {
        "type": "text"
      },
      "state": {
        "localValue": "{{value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Text": {
      "element": "textarea",
      "props": {
        "rows": 3
      },
      "state": {
        "localValue": "{{value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Long Text": {
      "element": "textarea",
      "props": {
        "rows": 6
      },
      "state": {
        "localValue": "{{value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Int": {
      "element": "input",
      "props": {
        "type": "number"
      },
      "state": {
        "localValue": "{{value || 0}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "transform": "parseInt",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Float": {
      "element": "input",
      "props": {
        "type": "number",
        "step": "0.01"
      },
      "state": {
        "localValue": "{{value || 0}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "transform": "parseFloat",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Currency": {
      "element": "input",
      "props": {
        "type": "number",
        "step": "0.01"
      },
      "state": {
        "localValue": "{{value || 0}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "transform": "parseFloat",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Check": {
      "element": "input",
      "props": {
        "type": "checkbox",
        "checked": "{{value || false}}",
        "disabled": "{{readOnly}}"
      },
      "state": {
        "localValue": "{{value || false}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "extract": "checked",
          "delegate": "onChange"
        }
      }
    },
    "Date": {
      "element": "input",
      "props": {
        "type": "date"
      },
      "state": {
        "localValue": "{{value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        }
      }
    },
    "Datetime": {
      "element": "input",
      "props": {
        "type": "datetime-local"
      },
      "state": {
        "localValue": "{{value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        }
      }
    },
    "Time": {
      "element": "input",
      "props": {
        "type": "time"
      },
      "state": {
        "localValue": "{{value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        }
      }
    },
    "Select": {
      "element": "select",
      "props": {
        "disabled": "{{readOnly}}"
      },
      "state": {
        "localValue": "{{value}}"
      },
      "children": [
        {
          "element": "option",
          "props": {
            "value": ""
          },
          "content": ""
        },
        {
          "repeat": "{{(field.options || '').split('\\n').filter(o => o.trim())}}",
          "element": "option",
          "props": {
            "value": "{{item}}"
          },
          "content": "{{item}}"
        }
      ],
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        }
      }
    },
    "Section Break": {
      "layoutOnly": true
    },
    "Tab Break": {
      "layoutOnly": true
    },
    "Column Break": {
      "layoutOnly": true
    },
    "Code": {
      "element": "textarea",
      "props": {
        "rows": 10,
        "className": "{{CWStyles.field.code}}"
      },
      "state": {
        "localValue": "{{typeof value === 'object' && value !== null ? JSON.stringify(value, null, 2) : (value || '')}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue"
        },
        "onBlur": {
          "delegate": "onBlur"
        },
        "onKeyDown": {
          "custom": true
        }
      }
    },
    "Attach Image": {
      "customComponent": true
    },
    "Percent": {
      "element": "input",
      "props": {
        "type": "number",
        "step": "0.01",
        "min": "0",
        "max": "100"
      },
      "state": {
        "localValue": "{{value === null || value === undefined ? '' : value}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "transform": "parseFloat",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      },
      "suffix": "%"
    },
    "Text Editor": {
      "element": "textarea",
      "props": {
        "rows": 10,
        "className": "{{CWStyles.field.textarea}}"
      },
      "state": {
        "localValue": "{{value || ''}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Password": {
      "element": "input",
      "props": {
        "type": "password",
        "autoComplete": "current-password"
      },
      "state": {
        "localValue": "{{value || ''}}"
      },
      "events": {
        "onChange": {
          "updateState": "localValue",
          "delegate": "onChange"
        },
        "onBlur": {
          "delegate": "onBlur"
        }
      }
    },
    "Read Only": {
      "element": "input",
      "props": {
        "type": "text",
        "readOnly": true,
        "className": "{{CWStyles.field.input + ' ' + CWStyles.input.readOnly}}"
      },
      "state": {
        "localValue": "{{value || ''}}"
      },
      "events": {}
    },
    "HTML": {
      "layoutOnly": true
    },
    "Button": {
      "customComponent": true
    },
    "Link": {
      "customComponent": true
    }
  },
  "elementDefaults": {
    "input": {
      "className": "{{CWStyles.field.input}}",
      "readOnly": "{{readOnly}}",
      "placeholder": "{{field.placeholder}}"
    },
    "textarea": {
      "className": "{{CWStyles.field.textarea}}",
      "readOnly": "{{readOnly}}",
      "placeholder": "{{field.placeholder}}"
    },
    "select": {
      "className": "{{CWStyles.field.select}}",
      "disabled": "{{readOnly}}"
    }
  },
