

{
  "debug": true,
  "adapters": {
    "defaults": {
      "db": "pocketbase",
      "auth": "auth",
      "storage": null,
      "email": null
    },
    "registry": {
      "pocketbase": {
        "type": "db",
        "name": "PocketBase",
        "description": "PocketBase cloud database",
        "handler": "_dbAdapters.pocketbase",
        "capabilities": [
          "select",
          "create",
          "update",
          "delete"
        ],
        "config": {
          "url": "http://127.0.0.1:8090",
          "collection": "item"
        }
      },
      "memory": {
        "type": "db",
        "name": "Memory",
        "description": "In-memory storage (volatile)",
        "handler": "_dbAdapters.memory",
        "capabilities": [
          "select",
          "create",
          "update",
          "delete"
        ],
        "config": {
          "maxRecords": 10000
        }
      },
      "storage": {
        "type": "db",
        "name": "Local Storage",
        "description": "Browser localStorage persistence",
        "handler": "_dbAdapters.storage",
        "capabilities": [
          "select",
          "create",
          "update",
          "delete"
        ],
        "config": {
          "prefix": "coworker_",
          "maxSize": 5242880
        }
      },
      "jwt": {
        "type": "auth",
        "name": "JWT Auth",
        "description": "JSON Web Token authentication",
        "handler": "_authAdapters.jwt",
        "capabilities": [
          "register",
          "login",
          "logout",
          "refresh",
          "verify",
          "change_password"
        ],
        "config": {}
      }
    }
  },
  "auth": {
    "jwtSecret": "change-this-secret-in-production",
    "jwtAlgorithm": "HS256",
    "accessTokenExpiry": "15m",
    "refreshTokenExpiry": "30d",
    "accessTokenExpiryMs": 900000,
    "refreshTokenExpiryMs": 2592000000,
    "passwordHashIterations": 100000,
    "saltLength": 16,
    "maxFailedAttempts": 5,
    "lockDurationMs": 900000,
    "maxRefreshTokens": 5,
    "userDoctype": "User",
    "userEmailField": "email",
    "defaultRoles": [
      "Desk User"
    ],
    "adminRole": "System Manager",
    "publicRole": "Is Public"
  },
